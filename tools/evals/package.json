{
  "name": "duopulse-evals",
  "version": "1.0.0",
  "description": "Pattern evaluation tools for DuoPulse v5",
  "type": "module",
  "scripts": {
    "generate": "node generate-patterns.js",
    "evaluate": "node evaluate-expressiveness.js",
    "serve": "npx serve public -l 3000",
    "build": "npm run generate && npm run evaluate",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "devDependencies": {
    "serve": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
